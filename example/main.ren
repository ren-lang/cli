import pkg "ren/array" as Array
import pkg "ren/console" exposing { log }
import pkg "ren/object" exposing { construct }

import ext "chalk" as Chalk

pub let main = [ name ] => {
    let red = Chalk.default.red
    run log `Hi there ${red name}, Ren says hello!`

    ret Array.map (_ + 1) [1, 2, 3, 4, 5]
}